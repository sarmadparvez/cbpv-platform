{
  "name": "mat-forty-two",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve -c development",
    "build": "ng build",
    "test": "ng test",
    "test:single-run": "ng test --watch=false",
    "eslint:fix": "eslint \"src/{app,environments,assets}/**/*{.ts,.js}\" --fix",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "genpage": "node ./scripts/generatePage",
    "postgenpage": "ng lint --fix",
    "lint": "ng lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix format:check lint:fix lint:check"
    }
  },
  "private": true,
