{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c production",
    "start:dev": "ng serve -c development",
    "build": "ng build -c production",
    "test": "ng test",
    "test:single-run": "ng test --watch=false",
    "eslint:fix": "eslint \"src/{app,environments,assets}/**/*{.ts,.js}\" --fix",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "postgenpage": "ng lint --fix",
    "lint": "ng lint",
    "lint:format:fix": "npm run lint:fix && npm run format:fix",
    "generate:api": "openapi-generator-cli generate -i ../admin/swagger/swagger.json -g typescript-angular -o gen/api/admin/ -p=removeOperationIdPrefix=true && openapi-generator-cli generate -i ../task/swagger/swagger.json -g typescript-angular -o gen/api/task/ -p=removeOperationIdPrefix=true",
    "generate:api:node": "openapi-generator-cli generate -i ../admin/swagger/swagger.json -g typescript-node -o gen/api/node/admin/ -p=removeOperationIdPrefix=true "
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular/animations": "^13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/material-moment-adapter": "^13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@casl/ability": "^5.4.3",
    "@kolkov/ngx-gallery": "^2.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@swimlane/ngx-charts": "^20.0.1",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^3.1.2",
    "ngx-avatars": "^1.2.0",
    "ngx-material-rating": "^2.0.0",
    "ngx-spinner": "^13.0.0",
    "rxjs": "~7.5.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/language-service": "~13.1.1",
    "@openapitools/openapi-generator-cli": "^2.4.23",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "husky": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "protractor": "^7.0.0",
    "scripty": "^1.9.1",
    "ts-node": "~7.0.0",
    "typescript": "^4.5.4"
  }
}
